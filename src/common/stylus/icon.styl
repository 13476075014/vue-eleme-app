    url('fonts/vue-eleme-app-icon.svg?iqm4fh#vue-eleme-app-icon') format('svg')
  font-weight: normal
  font-style: normal


[class^="icon-"], [class*=" icon-"]
  /* use !important to prevent issues with browser extensions that change fonts */
